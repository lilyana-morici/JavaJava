    <Button text="" onAction="#onButtonClick" fx:id="button03"/>
    <Button text="" onAction="#onButtonClick" fx:id="button04"/>

    <Button text="" onAction="#onButtonClick" fx:id="button10"/>
    <Button text="" onAction="#onButtonClick" fx:id="button11"/>
    <Button text="" onAction="#onButtonClick" fx:id="button12"/>
